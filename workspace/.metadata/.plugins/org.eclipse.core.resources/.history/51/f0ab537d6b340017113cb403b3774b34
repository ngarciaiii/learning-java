package week2;

import java.util.Scanner;

public class week2_ex39_42{
	
//	public static void printStars(String star) {
//		star = "*";
//		System.out.print(star);
//	}
	
	private static void printStars(int amount) {
		int i = 0;
		while(i < amount) {
			System.out.print("*");
			i++;
		}
	}
	
	public static void main(String[] args) {
		printStars(3);
	}

}